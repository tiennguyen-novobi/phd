<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="inherit_view_stock_quantity_history" model="ir.ui.view">
        <field name="name">Inventory Report at Date</field>
        <field name="model">stock.quantity.history</field>
        <field name="inherit_id" ref="stock.view_stock_quantity_history"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='inventory_datetime']" position="replace">
                <field name="inventory_date"/>
            </xpath>
        </field>
    </record>
</odoo>
