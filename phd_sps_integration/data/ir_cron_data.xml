<?xml version='1.0' encoding='utf-8'?>
<odoo noupdate="1">
    <record id="ir_cron_phd_fetch_doc_form_sps" model="ir.cron">
        <field name="name">SPS Automation: Fetch Documents</field>
        <field name="model_id" ref="model_phd_sps_commerce_file"/>
        <field name="state">code</field>
        <field name="code">model._fetch_order()</field>
        <field name="interval_number">10</field>
        <field name="interval_type">minutes</field>
        <field name="doall" eval="True"/>
        <field name="numbercall">-1</field>
    </record>

