<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="view_res_partner_form_inherit_phd_mrp" model="ir.ui.view">
        <field name="name">res.partner.form.view.inherit.phd.mrp</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='misc']" position="after">
                <group name="subcontract" string="Subcontract">
                    <field string="Receipt Operation Type" name="picking_type_id"/>
                    <field string="Subcontracting Operation Type" name="subcontract_picking_type_id"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
