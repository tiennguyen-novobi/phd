<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="phd_marketing_order_stage_draft" model="phd.mko.stage">
            <field name="name">Draft</field>
            <field name="folded" eval="False"/>
            <field name="sequence" eval="1"/>
        </record>

        <record id="phd_marketing_order_stage_waiting_for_approval" model="phd.mko.stage">
            <field name="name">Waiting For Approval</field>
            <field name="folded" eval="False"/>
            <field name="sequence" eval="2"/>
        </record>

        <record id="phd_marketing_order_stage_changes_needed" model="phd.mko.stage">
            <field name="name">Changes Needed</field>
            <field name="folded" eval="False"/>
            <field name="allow_apply_change" eval="True"/>
            <field name="sequence" eval="3"/>
        </record>

        <record id="phd_marketing_order_stage_approval" model="phd.mko.stage">
            <field name="name">Approval</field>
            <field name="folded" eval="False"/>
            <field name="sequence" eval="4"/>
        </record>

        <record id="phd_marketing_order_stage_rejected" model="phd.mko.stage">
            <field name="name">Rejected</field>
            <field name="folded" eval="False"/>
            <field name="sequence" eval="5"/>
        </record>

        <record id="phd_marketing_order_stage_canceled" model="phd.mko.stage">
            <field name="name">Canceled</field>
            <field name="folded" eval="False"/>
            <field name="sequence" eval="6"/>
        </record>

        <record id="phd_marketing_order_stage_closed" model="phd.mko.stage">
            <field name="name">Closed</field>
            <field name="folded" eval="False"/>
            <field name="sequence" eval="7"/>
        </record>
    </data>
</odoo>