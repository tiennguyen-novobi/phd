        <record id="phd_purchase_order_stage_rfq" model="phd.purchase.order.stage">
            <field name="name">RFQ</field>
            <field name="sequence" eval="1"/>
        </record>

        <record id="phd_purchase_order_stage_rfq_sent" model="phd.purchase.order.stage">
            <field name="name">RFQ Sent</field>
            <field name="sequence" eval="2"/>
        </record>

        <record id="phd_purchase_order_stage_to_approve" model="phd.purchase.order.stage">
            <field name="name">To Approve</field>
            <field name="sequence" eval="3"/>
        </record>

        <record id="phd_purchase_order_stage_purchase_order" model="phd.purchase.order.stage">
            <field name="name">Purchase Order</field>
            <field name="sequence" eval="4"/>
        </record>

        <record id="phd_purchase_order_stage_on_hold" model="phd.purchase.order.stage">
            <field name="name">On Hold</field>
            <field name="sequence" eval="5"/>
        </record>

<!--        <record id="phd_purchase_order_stage_acknowledgment" model="phd.purchase.order.stage">-->
<!--            <field name="name">Acknowledgment</field>-->
<!--            <field name="sequence" eval="6"/>-->
<!--        </record>-->

        <record id="phd_purchase_order_stage_ready_pickup" model="phd.purchase.order.stage">
            <field name="name">Ready to Pickup</field>
            <field name="sequence" eval="7"/>
        </record>

        <record id="phd_purchase_order_stage_shipped" model="phd.purchase.order.stage">
            <field name="name">Shipped</field>
            <field name="sequence" eval="8"/>
        </record>

        <record id="phd_purchase_order_stage_locked" model="phd.purchase.order.stage">
            <field name="name">Closed</field>
            <field name="sequence" eval="9"/>
