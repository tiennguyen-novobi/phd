<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="phd_res_partner_form" model="ir.ui.view">
        <field name="name">phd.res.partner.view.form</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='property_supplier_payment_term_id']" position="after">
                <field name="is_subcontractor" readonly="1"/>
            </xpath>
        </field>
    </record>
</odoo>