<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="phd_res_config_settings_form_view" model="ir.ui.view">
            <field name="name">phd_res_config_settings_form_view</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="account.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <div id="bank_cash" position="after">
                    <t groups="account.group_account_user">
                        <h2>Braintree/Paypal Setting</h2>
                        <div class="row mt16 o_settings_container" name="main_paypal_setting_container">
                            <div class="col-12 col-lg-6 o_setting_box" id="main_paypal" groups="base.group_no_one">
                                <div class="o_setting_left_pane"/>
                                <div class="o_setting_right_pane">
                                    <div class="content-group">
                                        <div class="row mt16">
                                            <label for="braintree_merchant_id" class="col-lg-3 o_light_label"/>
                                            <field name="braintree_merchant_id" required="1" options="{'no_create_edit': True, 'no_open': True}" context="{'active_test': False}"/>
                                        </div>
                                        <div class="row mt16">
                                            <label for="braintree_public_key" class="col-lg-3 o_light_label"/>
                                            <field name="braintree_public_key" required="1" options="{'no_create_edit': True, 'no_open': True}" context="{'active_test': False}"/>
                                        </div>
                                        <div class="row mt16">
                                            <label for="braintree_private_key" class="col-lg-3 o_light_label"/>
                                            <field name="braintree_private_key" required="1" options="{'no_create_edit': True, 'no_open': True}" context="{'active_test': False}"/>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-lg-6 o_setting_box" id="overview_paypal">
                                <div class="o_setting_left_pane"/>
                                <div class="o_setting_right_pane">
                                    <div class="content-group">
                                        <div class="row mt16">
                                            <label for="paypal_journal_id" class="col-lg-3 o_light_label"/>
                                            <field name="paypal_journal_id" required="1" options="{'no_create_edit': True, 'no_open': True}" context="{'active_test': False}"/>
                                        </div>
                                        <div class="row mt16">
                                            <label for="paypal_merchant" class="col-lg-3 o_light_label"/>
                                            <field name="paypal_merchant" required="1" options="{'no_create_edit': True, 'no_open': True}" context="{'active_test': False}"/>
                                        </div>
                                        <div class="row mt16">
                                            <label for="paypal_sales_account_id" class="col-lg-3 o_light_label"/>
                                            <field name="paypal_sales_account_id" required="1" options="{'no_create_edit': True, 'no_open': True}" context="{'active_test': False}"/>
                                        </div>
                                        <div class="row mt16">
                                            <label for="paypal_fee_account_id" class="col-lg-3 o_light_label"/>
                                            <field name="paypal_fee_account_id" required="1" options="{'no_create_edit': True, 'no_open': True}" context="{'active_test': False}"/>
                                        </div>
                                        <div class="row mt16">
                                            <label for="paypal_bank_account_id" class="col-lg-3 o_light_label"/>
                                            <field name="paypal_bank_account_id" required="1" options="{'no_create_edit': True, 'no_open': True}" context="{'active_test': False}"/>
                                        </div>
                                        <div class="row mt16">
                                            <label for="paypal_analytic_account_id" class="col-lg-3 o_light_label"/>
                                            <field name="paypal_analytic_account_id" required="1" options="{'no_create_edit': True, 'no_open': True}" context="{'active_test': False}"/>
                                        </div>
                                        <div class="row mt16">
                                            <label for="paypal_analytic_tag_ids" class="col-lg-3 o_light_label"/>
                                            <field name="paypal_analytic_tag_ids" required="1" widget="many2many_tags" options="{'no_create_edit': True, 'no_open': True}" context="{'active_test': False}"/>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </t>
                </div>
            </field>
    </record>
</odoo>