<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="phd_sale_order_stage_quotation" model="phd.sale.order.stage">
            <field name="name">Quotation</field>
            <field name="sequence" eval="1"/>
        </record>

        <record id="phd_sale_order_stage_quotation_sent" model="phd.sale.order.stage">
            <field name="name">Quotation Sent</field>
            <field name="sequence" eval="2"/>
        </record>

        <record id="phd_sale_order_stage_sale_order" model="phd.sale.order.stage">
            <field name="name">Sales Order</field>
            <field name="sequence" eval="3"/>
        </record>

        <record id="phd_sale_order_stage_on_hold" model="phd.sale.order.stage">
            <field name="name">On Hold</field>
            <field name="sequence" eval="4"/>
        </record>

        <record id="phd_sale_order_stage_partially_shipped" model="phd.sale.order.stage">
            <field name="name">Partially Shipped</field>
            <field name="sequence" eval="5"/>
        </record>

        <record id="phd_sale_order_stage_fully_shipped" model="phd.sale.order.stage">
            <field name="name">Fully Shipped</field>
            <field name="sequence" eval="6"/>
        </record>

        <record id="phd_sale_order_stage_closed" model="phd.sale.order.stage">
            <field name="name">Closed</field>
            <field name="sequence" eval="7"/>
        </record>

        <record id="phd_sale_order_stage_cancelled" model="phd.sale.order.stage">
            <field name="name">Cancelled</field>
            <field name="sequence" eval="8"/>
        </record>
    </data>
</odoo>