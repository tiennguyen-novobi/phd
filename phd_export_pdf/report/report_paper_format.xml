<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="phd_report_paperformat" model="report.paperformat">
            <field name="name">PHD Report Format</field>
            <field name="default" eval="True"/>
            <field name="format">custom</field>
            <field name="page_height">260</field>
            <field name="page_width">300</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">10</field>
            <field name="margin_bottom">15</field>
            <field name="margin_left">5</field>
            <field name="margin_right">5</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">35</field>
            <field name="dpi">120</field>
        </record>

        <record id="phd_report_paperformat_with_company_info" model="report.paperformat">
            <field name="name">PHD Purchase Order Format</field>
            <field name="default" eval="True"/>
            <field name="format">custom</field>
            <field name="page_height">260</field>
            <field name="page_width">300</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">40</field>
            <field name="margin_bottom">15</field>
            <field name="margin_left">5</field>
            <field name="margin_right">5</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">35</field>
            <field name="dpi">120</field>
        </record>
    </data>
</odoo>