<odoo>
    <data noupdate="1">
        <record forcecreate="True" id="adjust_percentage" model="decimal.precision">
            <field name="name">Adjust Percentage</field>
            <field name="digits">3</field>
        </record>

        <record id="seq_replenishment_history" model="ir.sequence">
            <field name="name">Replenishment History</field>
            <field name="code">replenishment.history</field>
            <field name="implementation">standard</field>
            <field name="active">True</field>
            <field name="prefix">RH</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="padding">5</field>
            <field name="use_date_range">False</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
