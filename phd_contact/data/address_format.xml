<odoo>
    <data noupdate="1">
         <function name="write" model="ir.model.data">
            <function name="search" model="ir.model.data">
                <value eval="[('module', '=', 'base'), ('name', 'in', ['us'])]"/>
            </function>
            <value eval="{'noupdate': False}"/>
        </function>

        <record id="base.us" model="res.country">
            <field eval="'%(street)s\n%(street2)s\n%(city)s, %(state_code)s %(zip)s\n%(country_name)s'"
                   name="address_format"/>
        </record>

        <function name="write" model="ir.model.data">
            <function name="search" model="ir.model.data">
                <value eval="[('module', '=', 'base'), ('name', 'in', ['us'])]"/>
            </function>
            <value eval="{'noupdate': True}"/>
        </function>
    </data>
</odoo>