<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_contact_partner_info_form" model="ir.ui.view">
            <field name="name">res.contact.partner.info.form</field>
            <field name="model">res.partner</field>
            <field eval="8" name="priority"/>
            <field name="arch" type="xml">
                <form string="Contacts">
                    <group>
                        <field name="name" string="Name"/>
                        <field name="email"/>
                    </group>

                    <footer>
                        <button name="preference_save" type="object" string="Save" class="btn-primary"/>
                        <button name="preference_cancel" string="Cancel" special="cancel" class="btn-secondary"/>
                    </footer>
                </form>
            </field>
        </record>

    </data>
</odoo>
